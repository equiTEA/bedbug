{
  "name": "nextjs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "lint": "next lint",

    "dev": "wait-on http-get://localhost:3000 && next dev -p 3001",
    "dev:local": "wait-on http-get://localhost:3000 &&../../node_modules/.bin/dotenv -e ../../.env.local next dev -- -p 3001",
    
    "build:staging": "../../node_modules/.bind/dotenv -e ../../.env.staging next build",
    "start:staging": "../../node_modules/.bin/dotenv -e ../../.env.staging -- next start -p 3001",

    "build:prod": "../../node_modules/.bind/dotenv -e ../../.env.prod next build",
    "start:prod": "../../node_modules/.bin/dotenv -e ../../.env.prod -- next start -p 3001"
  },
  "dependencies": {
    "@bedbug/hooks": "*",
    "@bedbug/networking": "*",
    "@bedbug/ui": "*",
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@mapbox/search-js-core": "^1.0.0-beta.13",
    "@mapbox/search-js-web": "^1.0.0-beta.13",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.2",
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.31.1",
    "use-state-if-mounted": "^1.0.7",
    "validator": "^13.7.0",
    "wait-on": "^6.0.1"
