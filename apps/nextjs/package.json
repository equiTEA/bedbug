    "@bedbug/types": "*",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.3",
    "@types/node": "^17.0.12",
    "@types/react": "18.0.17",
    "@types/validator": "^13.7.7",
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "next-transpile-modules": "9.0.0",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  }
