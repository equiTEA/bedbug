{
  "name": "monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "package": "turbo run package",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:local": "turbo run dev:local --parallel"
  },
  "devDependencies": {
    "dotenv-cli": "^6.0.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
